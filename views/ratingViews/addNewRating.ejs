<body>
    <div class="center">
    <h3>Add new rating</h3>
    <form action="add" method="POST">
        <div class="field">
            <div class="label" for="song_id">Song Id</div>
            <input type="text" name="song_id" placeholder="type" />
        </div>
        <div class="field">
            <div class="label" for="rating">Rating</div>
            <input type="text" name="rating" placeholder="type"/>
        </div>
        <button class="btn" type="submit">Add</button>
    </form>
    </div>
</body>