<div id="dashboard">
    <img id="background-image" src="../../images/eguitar.jpg">
    <div id="album-box" class="content-box">
        <div id="album-name">Manage Songs</div>
        <button id = "song-add-queue" onclick="navigateTo('/admin/song/add')">Add A Song</button>
        <hr>
        <% songs.forEach(song => { %>
            <div class="list-item-song" onclick = "navigateTo('/admin/song/<%=song.song_id%>')">
                <img class="list-song-cover" src="../../<%= song.album_image%>">
                <div class="list-song-title"><%= song.song_name%></div>
                <div class="list-song-artist"><%=song.artist_name%></div>
                <div class="list-song-album"><%=song.album_name%></div>
                <div class="list-song-duration"><%= song.duration %></div>
                <div class="list-song-options">
                    <i class="fas fa-trash-alt" onclick="deleteSong('<%= song.song_id %>')"></i>
                </div>
            </div>

        <% }) %>
        <hr>
    </div>
</div>